<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.ops4j</groupId>
    <artifactId>base</artifactId>
    <version>1.2.3</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <groupId>org.ops4j.base</groupId>
  <artifactId>ops4j-base-lang</artifactId>
  <version>1.2.3</version>

  <name>OPS4J Base - Lang</name>
  <description>
    OPS4J Base - Utilities/Extensions related to java.lang.
  </description>

  <properties>
    <bundle.symbolicName>org.ops4j.base.lang</bundle.symbolicName>
    <bundle.namespace>org.ops4j.lang</bundle.namespace>
  </properties>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.felix</groupId>
        <artifactId>maven-bundle-plugin</artifactId>
      </plugin>
    </plugins>
  </build>
  
  <distributionManagement>
    <repository>
      <id>nexus-test-harness-repo</id>
      <url>${nexus-base-url}content/repositories/p2r01</url>
    </repository>
  </distributionManagement>

</project>