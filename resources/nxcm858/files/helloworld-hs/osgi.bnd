Bundle-Activator:\
  ${bundle.namespace}.internal.Activator

Import-Package:\
  org.osgi.framework;version="[1.0.0,2.0.0)",\
  org.osgi.service.http;version="[1.0.0,2.0.0)",\
  *

Export-Package:\
  !*

Private-Package:\
  ${bundle.namespace}.internal.*

-removeheaders:\
  Private-Package,\
  Include-Resource,\
  Embed-Dependency,\
  Embed-Transitive